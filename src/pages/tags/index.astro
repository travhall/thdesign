---
import Layout from "../../layouts/Layout.astro";
const allProjects = await Astro.glob("../projects/*.mdx");
const tags = [
  ...new Set(allProjects.map((project) => project.frontmatter.tags).flat()),
];
const title = "All Project Tags";
---

<Layout title={title}>
  <section class="about-me p-6 lg:p-12 xl:p-16 my-8">
    <div>{tags.map((tag) => <p>{tag}</p>)}</div>
  </section>
</Layout>
