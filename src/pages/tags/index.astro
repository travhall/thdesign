---
import Layout from "../../layouts/Layout.astro";
const allProjects = await Astro.glob("../projects/*.mdx");
const tags = [
  ...new Set(allProjects.map((project) => project.frontmatter.tags).flat()),
];
const title = "All Project Tags";
---

<Layout title={title}>
  <section class="p-6 lg:p-12 xl:p-16 my-8" id="all-tags-section">
    {tags.map((tag) => <p class="font-heading mb-8">{tag}</p>)}
  </section>
</Layout>
